    "ext": "js, jsx, ts, tsx, json, mjs"
  },
  "jest": {
    "testTimeout": 10000,
    "modulePathIgnorePatterns": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/dist/test-utils/test-setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/dist/test-utils/test-setup-per-file.js"
    ],
