  "name": "api",
  "version": "1.1.3",
  "description": "The backend API for An Scéalaí.",
  "main": "server.ts",
  "scripts": {
    "test": "jest",
    "dev": "nodemon dist/server.js",
    "start": "node dist/server.js",
    "start:prod": "DB=an-scealai PORT=40000 NODE_ENV=prod node dist/server.js",
    "start:qa": "DB_HOSTNAME=localhost:27018/ DB=qa PORT=4002 node dist/server.js",
